<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="539.0" prefWidth="618.0" style="-fx-background-color: #800080;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.UpdateVote">
    <children>
        <TextField fx:id="TFIdVotant" layoutX="76.0" layoutY="92.0" promptText="IdJury" style="-fx-font-size: 14px; -fx-background-color: #FFFFFF;" />
        <TextField fx:id="TFIdProjet" layoutX="76.0" layoutY="164.0" promptText="IdProjet" style="-fx-font-size: 14px; -fx-background-color: #FFFFFF;" />
        <TextField fx:id="TFIdHackathon" layoutX="76.0" layoutY="233.0" promptText="IdHackathon" style="-fx-font-size: 14px; -fx-background-color: #FFFFFF;" />
        <TextField fx:id="TFIdEvaluation" layoutX="76.0" layoutY="306.0" promptText="IdEvaluation" style="-fx-font-size: 14px; -fx-background-color: #FFFFFF;" />
        <TextField fx:id="TFValeurVote" layoutX="76.0" layoutY="369.0" promptText="ValeurVote" style="-fx-font-size: 14px; -fx-background-color: #FFFFFF;" />
        <DatePicker fx:id="TFDate" layoutX="79.0" layoutY="421.0" style="-fx-font-size: 14px;" />
        <Button layoutX="335.0" layoutY="471.0" onAction="#updateVote" style="-fx-background-color: #4fa3f7; -fx-text-fill: #FFFFFF; -fx-font-size: 14px;" text="Update Vote" />

        <!-- Back button -->
        <Button layoutX="473.0" layoutY="471.0" onAction="#backToAfficherVote" style="-fx-background-color: #4fa3f7; -fx-text-fill: #FFFFFF; -fx-font-size: 14px;" text="Back" />
    </children>
</AnchorPane>
